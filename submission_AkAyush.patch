+                                    <View style={{ height: 25, width: 25, borderRadius: 15, backgroundColor: 'green', position: 'absolute', bottom: -12, alignSelf: 'center', alignItems: 'center', justifyContent: 'center' }}>
+                                        <PlusIcon width={25} height={25} />
+                                    </View>
